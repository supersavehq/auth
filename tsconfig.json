{
  "extends": "@tsconfig/node16-strictest",
  "compilerOptions": {
    "target": "ES6",
    "outDir": "./build",
    "declaration": true,
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "noImplicitThis": true,
    "moduleResolution": "node"
  },
  "exclude": ["./build"]
}
